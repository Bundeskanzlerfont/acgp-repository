<?xml version="1.0" encoding="windows-1252"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
<!-- El archivo faces-config.xml está pensado para registrar las reglas de navegación entre las
    páginas web de la aplicación JSF. -->

  <application>
    <default-render-kit-id>oracle.adf.core</default-render-kit-id>
  
        <locale-config>
      <default-locale>es_CL</default-locale>
      <supported-locale>es_CL</supported-locale>
    </locale-config>
    
  </application>
  
  <navigation-rule>
    
    <from-view-id>*</from-view-id>
    
    <navigation-case>
      <from-outcome>excelVisor</from-outcome>
      <to-view-id>/infraestructura/ExcelVisor.jsp</to-view-id>
      <redirect/>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>Error</from-outcome>
      <to-view-id>/infraestructura/Error.jspx</to-view-id>
      <redirect/>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>startHere</from-outcome>
      <to-view-id>/infraestructura/StartHere.jspx</to-view-id>
      <redirect/>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>bienvenida</from-outcome>
      <to-view-id>/Bienvenida.jspx</to-view-id>
      <redirect/>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>detalleGastosNomina</from-outcome>
      <to-view-id>/app/envionominas/detalleGastosNomina.jspx</to-view-id>
      <redirect/>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>mantenedorBancoProceso</from-outcome>
      <to-view-id>/app/envionominas/mantenedorBancoProceso.jspx</to-view-id>
      <redirect/>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>detalleExtraccionNomina</from-outcome>
      <to-view-id>/app/envionominas/detalleExtraccionNomina.jspx</to-view-id>
      <redirect/>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>ExtraccionNomina</from-outcome>
      <to-view-id>/app/envionominas/extraccionNomina.jspx</to-view-id>
      <redirect/>
    </navigation-case>

    <navigation-case>
      <from-outcome>dialog:PopupExtraerNomina</from-outcome>
      <to-view-id>/app/envionominas/popupExtraerNomina.jspx</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>ConsultaGastosNomina</from-outcome>
      <to-view-id>/app/envionominas/consultaGastosNomina.jspx</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>ArchivosNominas</from-outcome>
      <to-view-id>/app/envionominas/archivosNominas.jspx</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
  
 

</faces-config>