<?xml version="1.0" encoding="windows-1252"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">

<!-- El archivo faces-config-cv.xml está pensado para registrar los validadores y convertidores 
    que utilizará la aplicación JSF. -->
  <converter>
    <converter-id>UpperCaseConverter</converter-id>
    <converter-class>cl.bicevida.core.view.jsf.converters.UpperCaseConverter</converter-class>
  </converter>
  
  <validator>
    <validator-id>RutValidator</validator-id>
    <validator-class>cl.bicevida.core.view.jsf.validators.RutValidator</validator-class>
  </validator>
  <validator>
    <validator-id>NumberValidator</validator-id>
    <validator-class>cl.bicevida.envionominas.view.jsf.validators.NumberValidator</validator-class>
  </validator>
</faces-config>